{
    "name": "pcm-to-mp3-wasm",
    "version": "0.1.0",
    "description": "Minimal FFmpeg WASM for PCM to MP3 conversion (~1MB). A stripped-down build of ffmpeg.wasm optimized for audio encoding.",
    "main": "./dist/umd/ffmpeg-core.js",
    "exports": {
        ".": {
            "import": "./dist/esm/ffmpeg-core.js",
            "require": "./dist/umd/ffmpeg-core.js"
        },
        "./wasm": {
            "import": "./dist/esm/ffmpeg-core.wasm",
            "require": "./dist/umd/ffmpeg-core.wasm"
        }
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ffmpegwasm/ffmpeg.wasm.git"
    },
    "keywords": [
        "ffmpeg",
        "WebAssembly",
        "wasm",
        "audio",
        "mp3",
        "pcm",
        "encoder",
        "transcode",
        "lame"
    ],
    "author": "",
    "license": "GPL-2.0-or-later",
    "bugs": {
        "url": "https://github.com/ffmpegwasm/ffmpeg.wasm/issues"
    },
    "engines": {
        "node": ">=16.x"
    },
    "homepage": "https://github.com/ffmpegwasm/ffmpeg.wasm#readme",
    "publishConfig": {
        "access": "public"
    }
}